<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8" />
        <title><%= title %></title>
        <link rel="stylesheet" href="/f5static/tree.css" type="text/css" charset="utf-8">
    </head>
    <body>
        <div class="wrapper">
            <form id="pickForm" action="/f5api/pack" method="post" accept-charset="utf-8">

                <div id="parent">parent folder</div>
                <div id="file-tree" class="file-tree">
                    <%- _htmltext %>
                </div>
                <p>
                    <input type="submit" value="pack">
                    <input type="reset" value='clear'>
                </p>
            </form>
            <form id="pickResultForm" action="/f5api/pack" method="post" accept-charset="utf-8">
                <div id="pick-file-tree" class="file-tree"></div>
                <div id="msg"></div>
            </form>
        </div>
        <div id="quit">Quit f5</div>
        <div class="footer">
            <i class="version">version: <%- version %></i>
            <p><a target="_blank" href="https://github.com/island205/f5">â˜žFork me at github</a></p>
        </div>
        <script src="/socket.io/socket.io.js"></script>
        <script src="/f5static/jquery-min.js"></script>
        <script type="text/javascript">
            var socket = io.connect(location.hostname);
            socket.on('reload', function (data) {
                window.location.reload();
            });
        </script>
        <script src="/f5static/tree.js"></script>
        <script src="/f5static/picker.js"></script>
    </body>
</html>
